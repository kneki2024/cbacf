      i.fa.fa-globe
      select#location
        option(value='1') United States
        option(value='1') South America
      button.adv-adv-button-edt.par-add-button(type="button") ADD
      button.adv-adv-button-edt.par-del-button(type="button") DELETE 
      button.adv-adv-button-edt.par-bak-button(type="button") BACK
